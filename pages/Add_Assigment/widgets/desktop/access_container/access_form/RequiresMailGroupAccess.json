  "sourceData": "{{ \n  (() => {\n    const seen = new Set();\n    return utils.getDictionaryByName(\"MailGroup\")\n      .filter(mail => mail.ItemName !== \"\")\n      .sort((a, b) => a.ItemName.split('@')[1].localeCompare(b.ItemName.split('@')[1]))\n      .filter(mail => {\n        if (seen.has(mail.ItemName)) return false;\n        seen.add(mail.ItemName);\n        return true;\n      })\n      .map(p => ({\n        label: p.ItemName,\n        value: p.ItemName\n      }));\n  })() \n}}",
